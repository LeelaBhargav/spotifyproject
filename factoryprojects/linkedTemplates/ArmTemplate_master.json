{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "factoryprojects"
		},
		"AzureSqlls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlls'"
		},
		"datalakels_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakels'"
		},
		"AzureSqlls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "leelaserver.database.windows.net"
		},
		"AzureSqlls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "leeladb"
		},
		"AzureSqlls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "leelaadmin"
		},
		"datalakels_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://leelastorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "factoryprojects_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlls_password": {
						"value": "[parameters('AzureSqlls_password')]"
					},
					"datalakels_accountKey": {
						"value": "[parameters('datalakels_accountKey')]"
					},
					"AzureSqlls_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlls_properties_typeProperties_server')]"
					},
					"AzureSqlls_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlls_properties_typeProperties_database')]"
					},
					"AzureSqlls_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlls_properties_typeProperties_userName')]"
					},
					"datalakels_properties_typeProperties_url": {
						"value": "[parameters('datalakels_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}